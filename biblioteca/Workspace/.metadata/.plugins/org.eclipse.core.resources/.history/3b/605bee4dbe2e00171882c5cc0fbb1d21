package modelo;

public class Busca 
{
	private int cod_busca;
	private List<> reservas;
	private List<Exemplar> exemplares;
	
	// --------------------Métodos--------------------------

	public Ex_Reservado (int cod_busca, List<Reserva> reservas, List<Exemplar> exemplares)
	{
		this.cod_busca = cod_busca;
		this.reservas = reservas;
		this.exemplares = exemplares;
	}
	
	//Set and Get
	public int getCod_busca() 
	{
		return cod_busca;
	}

	public void setCod_busca(int cod_busca)
	{
		this.cod_busca = cod_busca;
	}

	
	public List<Reserva> getReservas() 
	{
		return reservas;
	}

	public void setReservas(List<Reserva> reservas)
	{
		this.reservas = reservas;
	}

	public List<Exemplar> getExemplares() 
	{
		return exemplares;
	}

	public void setExemplares(List<Exemplar> exemplares)
	{
		this.exemplares = exemplares;
	}
}
